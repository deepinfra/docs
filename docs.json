{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "DeepInfra",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@deepinfra.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://deepinfra.com/dashboard"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "models"
            ]
          },
          {
            "group": "Chat Completions",
            "pages": [
              "chat/overview",
              "chat/streaming",
              "chat/structured-outputs",
              "chat/tool-calling",
              "chat/vision",
              "chat/prompt-caching",
              "chat/log-probs"
            ]
          },
          {
            "group": "More APIs",
            "pages": [
              "apis/completions",
              "apis/embeddings",
              "apis/image-generation",
              "apis/speech",
              "apis/deepinfra-native"
            ]
          },
          {
            "group": "Deploy Private Models",
            "pages": [
              "private-models/overview",
              "private-models/custom-llms",
              "private-models/lora",
              "private-models/lora-image"
            ]
          },
          {
            "group": "GPU Instances",
            "pages": [
              "gpu-instances/overview"
            ]
          },
          {
            "group": "Account & Security",
            "pages": [
              "account/authentication",
              "account/rate-limits",
              "account/webhooks",
              "account/okta-sso",
              "account/data-privacy",
              "account/subprocessors"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/langchain",
              "integrations/llama-index",
              "integrations/ai-sdk",
              "integrations/autogen"
            ]
          },
          {
            "group": "Tutorials",
            "pages": [
              "tutorials/stable-diffusion",
              "tutorials/whisper"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "https://api.deepinfra.com/openapi.json"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Models",
          "href": "https://deepinfra.com/models",
          "icon": "brain"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/deepinfra",
          "icon": "discord"
        }
      ]
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
